<?xml version="1.0" ?>
<options>
    <item name="first" type="UINT" num="1" value="1" help="The first MFCC to compute." />
    <item name="last" type="UINT" num="1" value="12" help="The last MFCC to compute." />
    <item name="floor" type="FLOAT" num="1" value="1.000000e-008" help="The minimum value allowed for melspectra when taking the log spectrum (this parameter will be forced to 1.0 when htkcompatible=1)." />
    <item name="lift" type="FLOAT" num="1" value="2.200000e+001" help="Parameter for cepstral 'liftering', set this to 0.0 to disable cepstral liftering." />
    <item name="htk" type="BOOL" num="1" value="false" help="Appends the 0-th coefficient at the end instead of placing it as the first element of the output vector." />
</options>
