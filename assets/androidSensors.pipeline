<?xml version="1.0" ?>
<pipeline ssi-v="1">
	
	<register>
		<load name="ssiioput"/>
	</register>
	
	<sensor create="AndSensors" >
		<output channel="accelerometer" pin="acc"/>
	</sensor>


	
	<consumer create="FileWriter" stream="true" path="./record/acc">
		<input pin="acc" frame ="1" />
	</consumer>
	
</pipeline>
